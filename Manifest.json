{
  "manifest_version": 2,
  "name": "Search helper",
  "version": "0.1",
  "description":"Brings all your website search bars to one place",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.5.1.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Search helper",
    "default_icon": "icon.png"
  },
  "icons": { "16": "icon.png",
    "48": "icon.png",
   "128": "icon.png" },
  "permissions": [
    "tabs",
    "storage"
  ]
}