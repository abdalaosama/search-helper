{
  "manifest_version": 2,
  "name": "Search is here",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.5.1.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Search helper"
  },
  "permissions": [
    "tabs",
    "storage"
  ]
}